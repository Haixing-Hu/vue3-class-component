{
  "opts": {
    "destination": "./doc/api",
    "recurse": true,
    "verbose": true,
    "encoding": "utf8",
    "template": "./node_modules/@qubit-ltd/jsdoc-minami"
  },
  "recurseDepth": 10,
  "source": {
    "include": ["./src", "./README.md"],
    "includePattern": ".+\\.js(doc|x)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "sourceType": "module",
  "plugins": [
    "plugins/markdown"
  ],
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc"]
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "default": {
      "outputSourceFiles": false
    }
  }
}
