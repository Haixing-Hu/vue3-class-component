{
  "name": "@qubit-ltd/vue3-class-component",
  "version": "2.4.0",
  "description": "A JavaScript class decorator for Vue3 components",
  "main": "dist/vue-class-component.min.cjs",
  "module": "dist/vue-class-component.min.mjs",
  "exports": {
    ".": {
      "require": "./dist/vue-class-component.min.cjs",
      "import": "./dist/vue-class-component.min.mjs"
    }
  },
  "files": [
    "dist/**/*",
    "doc/**/*",
    "LICENSE",
    "README.md",
    "README.zh_CN.md"
  ],
  "scripts": {
    "clean": "rm -rvf dist doc",
    "build": "cross-env NODE_ENV=production rollup -c rollup.config.mjs",
    "build:dev": "cross-env NODE_ENV=development rollup -c rollup.config.mjs",
    "build:all": "npm run clean && npm run build:dev && npm run build && npm run doc",
    "test": "jest",
    "lint": "eslint ./src ./test",
    "doc": "jsdoc -c jsdoc.json",
    "es5": "check-es-version -e 5 -s true",
    "deploy": "npm run lint && npm run test && npm run build:all && npm publish --registry='https://npm.qubit.ltd/' --access public"
  },
  "author": "Haixing Hu",
  "license": "Apache License 2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Haixing-Hu/vue3-class-component.git"
  },
  "bugs": {
    "url": "https://github.com/Haixing-Hu/vue3-class-component/issues"
  },
  "homepage": "https://github.com/Haixing-Hu/vue3-class-component",
  "keywords": [
    "vue",
    "vue3",
    "class",
    "decorator",
    "babel"
  ],
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "peerDependencies": {
    "@babel/runtime": "^7.26.0",
    "@qubit-ltd/clone": "^1.13.0",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/eslint-parser": "^7.27.0",
    "@babel/plugin-proposal-decorators": "^7.25.9",
    "@babel/plugin-transform-runtime": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "@babel/runtime": "^7.27.0",
    "@jest/core": "^29.7.0",
    "@qubit-ltd/clone": "^1.13.0",
    "@qubit-ltd/eslint-config": "^1.3.4",
    "@qubit-ltd/jsdoc-minami": "^1.5.2",
    "@qubit-ltd/naming-style": "^1.3.6",
    "@qubit-ltd/rollup-builder": "^1.8.9",
    "@qubit-ltd/type-detect": "^2.2.0",
    "@qubit-ltd/typeinfo": "^3.3.0",
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.3",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@vue/compiler-dom": "^3.5.13",
    "@vue/compiler-sfc": "^3.5.13",
    "@vue/server-renderer": "^3.5.13",
    "@vue/test-utils": "^2.4.6",
    "@vue/vue3-jest": "^29.2.6",
    "babel-jest": "^29.7.0",
    "check-es-version": "^1.5.1",
    "core-js": "^3.41.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-vue": "^9.33.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-environment-jsdom-global": "^4.0.0",
    "jest-extended": "^4.0.2",
    "jsdoc": "^4.0.4",
    "rollup": "^4.40.0",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-visualizer": "^5.14.0",
    "vue": "^3.5.13",
    "vue-template-compiler": "^2.7.16"
  },
  "packageManager": "yarn@4.5.0"
}
