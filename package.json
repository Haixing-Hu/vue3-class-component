{
  "name": "@haixing_hu/vue3-class-component",
  "version": "1.2.0",
  "description": "A JavaScript class decorator for Vue3 components",
  "type": "module",
  "main": "dist/vue3-class-component.cjs.min.js",
  "module": "dist/vue3-class-component.es.min.js",
  "exports": {
    ".": {
      "require": "./dist/vue3-class-component.cjs.min.js",
      "import": "./dist/vue3-class-component.es.min.js"
    }
  },
  "scripts": {
    "clean": "rm -rvf dist",
    "build:cjs": "cross-env NODE_ENV=production LIBRARY_TYPE=cjs rollup -c rollup.config.mjs",
    "build:cjs:dev": "cross-env NODE_ENV=development LIBRARY_TYPE=cjs rollup -c rollup.config.mjs",
    "build:es": "cross-env NODE_ENV=production LIBRARY_TYPE=es rollup -c rollup.config.mjs",
    "build:es:dev": "cross-env NODE_ENV=development LIBRARY_TYPE=es rollup -c rollup.config.mjs",
    "build": "yarn run build:cjs && yarn run build:es",
    "build:dev": "yarn run build:cjs:dev && yarn run build:es:dev",
    "test": "vitest run --coverage",
    "lint": "eslint ./src ./test",
    "doc": "gulp doc",
    "doc:server": "gulp doc:server",
    "analyze": "cross-env USE_ANALYZER=true NODE_ENV=production gulp build",
    "analyze:dev": "cross-env USE_ANALYZER=true NODE_ENV=development gulp build",
    "es5": "check-es-version -e 5 -s true",
    "deploy": "yarn && yarn lint && yarn test && yarn build:dev && yarn build && npm publish --registry='https://registry.npmjs.com/' --access public"
  },
  "author": "Haixing Hu",
  "license": "Apache License 2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Haixing-Hu/vue3-class-component.git"
  },
  "bugs": {
    "url": "https://github.com/Haixing-Hu/vue3-class-component/issues"
  },
  "homepage": "https://github.com/Haixing-Hu/vue3-class-component",
  "keywords": [
    "vue",
    "vue3",
    "class",
    "decorator",
    "babel"
  ],
  "browserify": {
    "transform": [
      "babelify"
    ]
  },
  "peerDependencies": {
    "vue": "^3.3.4"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/eslint-parser": "^7.22.15",
    "@babel/plugin-proposal-decorators": "^7.23.0",
    "@babel/plugin-transform-class-properties": "^7.22.5",
    "@babel/plugin-transform-runtime": "^7.22.15",
    "@babel/preset-env": "^7.22.20",
    "@babel/runtime": "^7.23.2",
    "@haixing_hu/eslint-config": "^1.0.4",
    "@haixing_hu/jsdoc-minami": "^1.4.0",
    "@haixing_hu/vite-plugin-babel": "^1.4.0",
    "@jest/core": "^29.7.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@vitejs/plugin-vue": "^4.4.0",
    "@vitest/coverage-istanbul": "^0.34.6",
    "@vue/compiler-dom": "^3.3.4",
    "@vue/compiler-sfc": "^3.3.4",
    "@vue/server-renderer": "^3.3.4",
    "@vue/test-utils": "^2.4.1",
    "@vue/vue3-jest": "^29.2.6",
    "babel-jest": "^29.7.0",
    "babel-loader": "^9.1.3",
    "check-es-version": "^1.4.2",
    "core-js": "^3.33.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.49.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-vue": "^9.17.0",
    "gulp": "^4.0.2",
    "gulp-connect": "^5.7.0",
    "gulp-eslint-new": "^1.8.3",
    "gulp-if": "^3.0.0",
    "gulp-jest": "^4.0.4",
    "gulp-jsdoc3": "^3.0.0",
    "happy-dom": "^12.9.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-environment-jsdom-global": "^4.0.0",
    "jest-environment-node": "^29.7.0",
    "jest-extended": "^4.0.1",
    "jsdoc": "^4.0.2",
    "rollup": "^4.0.2",
    "terser-webpack-plugin": "^5.3.9",
    "vite": "^4.4.11",
    "vitest": "^0.34.6",
    "vue": "^3.3.4",
    "vue-template-compiler": "^2.7.14",
    "webpack": "^5.88.2",
    "webpack-bundle-analyzer": "^4.9.1",
    "webpack-merge": "^5.9.0",
    "webpack-stream": "^7.0.0"
  },
  "packageManager": "yarn@3.6.3"
}
